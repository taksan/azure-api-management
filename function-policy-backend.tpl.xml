<policies>
    <inbound>
        <base />
        <set-backend-service backend-id="${backend-id}" />
        <rate-limit-by-key calls="100" renewal-period="10" counter-key="@(context.Subscription?.Key ?? "anonymous")" />

    </inbound>
    <backend>
        <base />
    </backend>
    <outbound>
        <base />
    </outbound>
    <on-error>
        <base />
    </on-error>
</policies>
